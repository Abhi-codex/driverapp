{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "InstaAid Driver",
    "slug": "driverapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "extra": {
      "eas": {
        "projectId": "3e15ce98-13c4-412b-b8a8-69a46ff33a7a"
      }
    },
    "android": {
      "package": "com.dhruva12.instaaid.driver",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "android.permission.RECEIVE_SMS",
        "android.permission.READ_SMS",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CALL_PHONE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBL8ds5GMZN_gXu9wjLbVIDSY_sPxgJL9Q"
        }
      },
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "bundleIdentifier": "com.dhruva12.instaaid.driver",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleMapsApiKey": "AIzaSyBL8ds5GMZN_gXu9wjLbVIDSY_sPxgJL9Q"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "@react-native-firebase/auth",
        {
          "android": {
            "requestVerificationCodeAutomatically": true,
            "forceRecaptchaFlowForTesting": false
          },
          "ios": {
            "requestVerificationCodeAutomatically": true
          }
        }
      ],
      "expo-location"
    ]
  }
}
